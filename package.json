{
  "name": "sapin",
  "license": "MIT",
  "author": "Stephane Bourbeau",
  "repository": {
    "type": "git",
    "url": "https://github.com/bourbest/sapin"
  },
  "version": "1.0.0",
  "description": "Create object validator using a declarative paradigm",
  "main": "./src/index.js",
  "modules": "./src/index.js",
  "scripts": {
    "lint": "eslint ./src/*.js",
    "test": "nyc mocha",
    "dev": "webpack --config ./webpack.config",
    "prod": "webpack --config ./webpack.config --env=production",
    "coverage": "nyc --require babel-register report --reporter=text-lcov | coveralls",
    "build": "npm run dev && npm run prod"
  },
  "peerDependencies": {
    "lodash": "^4.17.5"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-standard": "^1.0.2",
    "mocha": "^5.0.4",
    "nyc": "^11.4.1",
    "standard": "^11.0.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "redux-form",
    "validate"
  ],
  "bugs": "https://github.com/bourbest/sapin/issues",
  "dependencies": {
    "date-fns": "^1.29.0",
    "lodash": "^4.17.10"
  }
}
